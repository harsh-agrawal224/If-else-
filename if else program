import java.util.*;
class Ifelse{
	public static void main(String args[]){
		int age;
		System.out.print("Enter your age : ");
		Scanner s =new Scanner(System.in);
		age =s.nextInt();
		if (age>=18)
			System.out.println("Your are eligible");
		else
		System.out.println("You are not eligible");
		
	}
}
